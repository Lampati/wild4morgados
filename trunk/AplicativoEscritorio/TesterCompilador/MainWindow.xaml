<Window x:Class="TesterCompilador.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <StackPanel>
            <Grid>
                <TextBox IsEnabled="False" Name="textBoxDir" ContextMenu="{Binding null}" Margin="29,0,249,0"></TextBox>
                <Button Name="bttnDirTemporales" Click="bttnDirTemporales_Click" ContextMenu="{Binding null}" Margin="260,0,204,0">Ver</Button>

                <Button Name="bttnCompilarTodo" Click="bttnCompilarTodo_Click" ContextMenu="{Binding null}" Margin="415,0,6,0" IsEnabled="False">Compilar</Button>
            </Grid>
        </StackPanel>
        
    </Grid>
</Window>
