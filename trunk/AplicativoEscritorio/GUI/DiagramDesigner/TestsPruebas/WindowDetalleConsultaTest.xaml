<Window x:Class="DiagramDesigner.TestsPruebas.WindowDetalleConsultaTest"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:userControls="clr-namespace:DiagramDesigner.UserControls"
        Title="" Height="400" Width="450" ResizeMode="NoResize">
    <Grid Margin="5">
        <Grid.RowDefinitions>            
            <RowDefinition Height="150" />
            <RowDefinition Height="150" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto" />
            
        </Grid.ColumnDefinitions>

        <ScrollViewer Grid.Row="0" Margin="0,10,0,0">
            <StackPanel >            
                <TextBlock FontSize="15" Text="Variables de Entrada"></TextBlock>
                <userControls:VariablesConValoresPresenter Height="150" Width="410" HorizontalAlignment="Left" x:Name="dataVarsEntrada" EsVersionReducida="True" />
            </StackPanel>
        </ScrollViewer>

        <ScrollViewer Grid.Row="1" Margin="0,20,0,0">
            <StackPanel >
                <TextBlock FontSize="15" Text="Variables de Salida"></TextBlock>
                <userControls:VariablesConValoresPresenter Height="150" Width="410" HorizontalAlignment="Left" x:Name="dataVarsSalida" EsVersionReducida="True" />
            </StackPanel>
        </ScrollViewer>
    </Grid>
</Window>
